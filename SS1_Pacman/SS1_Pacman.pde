//Gaming's Biggest Characters: Pacman by Zachary Gerardi
void setup(){
  size(1080,720);
  strokeWeight(0);
  background(0);
}

void draw(){
  circle(width/2,height/2,250);
  fill(0);
  circle(525,290,50);
  fill(0);
  triangle(width/2,height/2,width/3,height/3,(width/3),(height/3)*2);
  fill(255);
  rectMode(CENTER);
  strokeWeight(1);
  rect(height/2,height/2,50,50);
  rect((height/2)-75,height/2,50,50);
  rect((height/2)-150,height/2,50,50);
  rect((height/2)-225,height/2,50,50);
  rect((height/2)-300,height/2,50,50);
  fill(255,255,0);

  

  
  
  
}
